{
  "name": "wiki-img-meta",
  "version": "0.0.0-development",
  "description": "Get Wikipedia Image Metadata",
  "main": "dist/index.js",
  "typings": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "clean": "rm -rf dist/**",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write \"**/*.{ts, js, json, md}\"",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:cov": "jest --coverage --ci",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release",
    "ci": "npm run lint && npm run format && npm run test:ci && npm run test:cov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abdmmar/wiki-img-meta.git"
  },
  "keywords": [
    "wikipedia",
    "image",
    "metadata"
  ],
  "files": [
    "dist"
  ],
  "author": "Abdullah Ammar <abd.ammar.mar@gmail.com> (https://abdmmar.tech/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abdmmar/wiki-img-meta/issues"
  },
  "homepage": "https://github.com/abdmmar/wiki-img-meta#readme",
  "devDependencies": {
    "@types/cheerio": "0.22.30",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.10",
    "@types/node-fetch": "2.5.12",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "4.3.8",
    "jest": "27.1.0",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "ts-jest": "27.0.5",
    "typescript": "4.4.2",
    "semantic-release": "^17.4.7"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.10",
    "node-fetch": "2.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
